CREATE TABLE Publisher (
publisher_id INT(100) AUTO_INCREMENT NOT NULL,
publisher_name VARCHAR(100),
PRIMARY KEY (publisher_id)
);
CREATE TABLE Library_items (
item_id INT(100) AUTO_INCREMENT NOT NULL,
publisher_id INT(100),
PRIMARY KEY (item_id),
FOREIGN KEY (publisher_id)
REFERENCES Publisher (publisher_id)
);
CREATE TABLE Library_user (
user_id INT(100) AUTO_INCREMENT NOT NULL,
user_name VARCHAR(100),
Email VARCHAR(100),
address VARCHAR(100),
PRIMARY KEY (user_id)


CREATE TABLE Author (
author_id INT(100) AUTO_INCREMENT NOT NULL,
author_name VARCHAR(100),
PRIMARY KEY (author_id)
);
CREATE TABLE HAS (
author_id INT(100),
item_id INT(100),
FOREIGN KEY (author_id)
REFERENCES Author (author_id),
FOREIGN KEY (item_id)
REFERENCES Library_items (item_id)
);
CREATE TABLE Uses (
user_id INT(100),
item_id INT(100),
FOREIGN KEY (user_id)
REFERENCES Library_user (user_id),
FOREIGN KEY (item_id)
REFERENCES Library_items (item_id)
);

CREATE TABLE Written_item (
item_id INT(100) NOT NULL,
user_id INT(100),
title VARCHAR(100),
written_item_type CHAR(1) NOT NULL,
number_pages INT(100),
release_data DATE,
PRIMARY KEY (item_id),
FOREIGN KEY (item_id)
REFERENCES Library_items (item_id),
FOREIGN KEY (user_id)
REFERENCES Library_user (user_id)
);
CREATE TABLE CD (
item_id INT(100) NOT NULL,
cd_name VARCHAR(100),
size INT(100),
PRIMARY KEY (item_id),
FOREIGN KEY (item_id)
REFERENCES Library_items (item_id)
);

